# Synthetic Data Generator Environment Configuration

# Generator Settings
GENERATOR_VERSION=v9
DATA_OUTPUT_DIR=data/generated
LOG_LEVEL=INFO

# Data Generation Parameters
START_DATE=2023-01-01
END_DATE=2024-12-31
FREQUENCY=1H  # 1H for hourly, 5T for 5-minute

# Quality Settings
VALIDATE_ON_GENERATE=true
SAVE_METADATA=true

# Performance
PARALLEL_PROCESSING=true
MAX_WORKERS=4

